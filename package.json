{
  "name": "ng-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run _server:run\" \"ng serve --progress=false --proxy-config proxy.conf.json\"",
    "build": "ng build && npm run _server:build",
    "build-universal": "ng build --prod && ng build --prod --app ng-starter --output-hashing=none",
    "test:client": "ng test",
    "lint:client": "ng lint",
    "lint:server": "tslint './server/**/*.ts' -c ./server/tslint.json --fix",
    "e2e": "ng e2e",
    "e2e:client": "ng e2e",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server"
  },
  "repository": {
    "url": "https://github.com/andantonyan/ng-starter"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.3.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/platform-server": "^4.3.4",
    "@angular/router": "^4.2.4",
    "@nguniversal/express-engine": "^1.0.0-beta.2",
    "body-parser": "~1.17.1",
    "compression": "^1.6.2",
    "cookie-parser": "~1.3.5",
    "core-js": "^2.4.1",
    "express": "^4.15.4",
    "express-jwt": "^5.1.0",
    "express-session": "^1.15.5",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "request-promise": "^4.2.1",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.3.0",
    "@angular/compiler-cli": "^4.2.4",
    "@angular/language-service": "^4.2.4",
    "@types/body-parser": "^1.16.3",
    "@types/compression": "^0.0.33",
    "@types/cookie-parser": "^1.4.0",
    "@types/cors": "^2.8.1",
    "@types/express": "^4.0.35",
    "@types/express-session": "^1.15.2",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/lodash": "^4.14.73",
    "@types/morgan": "^1.7.32",
    "@types/node": "~6.0.60",
    "@types/request-promise": "^4.1.36",
    "@types/winston": "^2.3.4",
    "codelyzer": "~3.1.1",
    "concurrently": "^3.5.0",
    "debug": "^3.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-remap-istanbul": "^0.2.1",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3",
    "webdriver-manager": "10.2.5",
    "winston": "^2.3.1"
  }
}
